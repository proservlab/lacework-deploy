{
  "context" :  {
    "global": {
      "environment"          : "target",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "iam" :  {
        "enabled" :  false,
        "user_policies_path" :  "${ iam_power_user_policy_path }",
        "users_path" :  "${ iam_users_path }"
      },
      "ec2" :  {
        "add_trusted_ingress" :  {
          "enabled"                     :  false,
          "trust_workstation_source"           :  false,
          "trust_attacker_source"       :  false,
          "trust_target_source"         :  false,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        }
      },
      "eks" :  {
        "add_iam_user_readonly_user": {
          "enabled": false,
          "iam_user_names": []
        },
        "add_iam_user_admin_user": {
          "enabled": false,
          "iam_user_names": []
        }
      }
    },
    "kubernetes" :  {
      "aws": {
        "app" :  {
          "enabled" :  false
        },
        "psp" :  {
          "enabled" :  false
        },
        "vulnerable" :  {
          "log4j_app" :  {
            "enabled" :  true
          },
          "voteapp" :  {
            "enabled" :  false
          },
          "rdsapp" :  {
            "enabled" :  false
          },
          "privileged_pod" :  {
            "enabled" :  false
          },
          "root_mount_fs_pod" :  {
            "enabled" :  false
          }
        }
      }
    }
  }
}