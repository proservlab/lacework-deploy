{
  "context" :  {
    "global": {
      "environment"          : "attacker",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "region"        : "us-east-1",
      "profile_name"  : "${ aws_profile }",
      "ec2" :  {
        "add_trusted_ingress" :  {
          "enabled"                     :  false,
          "trust_workstation"           :  false,
          "trust_attacker_source"       :  false,
          "trust_target_source"         :  false,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        }
      }
    }
  }
}