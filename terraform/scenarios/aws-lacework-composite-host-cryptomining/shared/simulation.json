{
  "context" :  {
    "global": {
      "environment"          : "${ environment }",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "enabled" : true,
      "ssm" :  {
        "target": {
          "execute": {
            "cpu_miner": {
              "enabled": true,
              "xmrig_version": "6.5.3",
              "minergate_server": "us-east.ethash-hub.miningpoolhub.com:20535",
              "minergate_user": "NOTAREALUSER",
              "attack_delay": 4500
            }
          }
        }
      }
    }
  }
}