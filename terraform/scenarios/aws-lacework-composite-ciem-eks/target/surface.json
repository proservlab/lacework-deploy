{
  "context" :  {
    "global": {
      "environment"          : "target",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "iam" :  {
        "enabled" :  true,
        "user_policies_path" :  "${ iam_power_user_policy_path }",
        "users_path" :  "${ iam_users_path }"
      },
      "ec2" :  {
        "add_trusted_ingress" :  {
          "enabled"                     :  false,
          "trust_workstation_source"           :  false,
          "trust_attacker_source"       :  false,
          "trust_target_source"         :  false,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        }
      },
      "ssm" :  {
        "aws_credentials" :  {
          "enabled" :  true,
          "compromised_keys_user" : "clue.burnetes@interlacelabs"
        },
        "ssh_keys" :  {
          "enabled" :  true
        }
      },
      "eks" :  {
        "add_iam_user_readonly_user": {
          "enabled": false,
          "iam_user_names": []
        },
        "add_iam_user_admin_user": {
          "enabled": true,
          "iam_user_names": [
            "clue.burnetes@interlacelabs"
          ]
        }
      }
    },
    "kubernetes" :  {
      "aws": {
        "reloader": {
          "enabled": true
        },
        "app" :  {
          "enabled" :  false,
          "service_port": 8000,
          "trust_attacker_source": true,
          "trust_workstation_source": true,
          "additional_trusted_sources": ["154.5.138.43/32"],
          "privileged": false,
          "enable_dynu_dns": false,
          "dynu_dns_domain": "${ dynu_dns_domain }"
        },
        "vulnerable" :  {
          "log4j_app" :  {
            "enabled" :  false,
            "enable_dynu_dns": false,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          },
          "voteapp" :  {
            "enabled" :  false
          },
          "rdsapp" :  {
            "enabled" :  false,
            "enable_dynu_dns": false,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          },
          "privileged_pod" :  {
            "enabled" :  false,
            "enable_dynu_dns": false,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          },
          "root_mount_fs_pod" :  {
            "enabled" :  false,
            "enable_dynu_dns": false,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          },
          "s3app" :  {
            "enabled" :  true,
            "service_port": 8000,
            "trust_attacker_source": true,
            "trust_workstation_source": true,
            "additional_trusted_sources": ["154.5.138.43/32"],
            "privileged": false,
            "enable_dynu_dns": true,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          },
          "app-auth" :  {
            "enabled" :  true,
            "service_port": 8000,
            "trust_attacker_source": true,
            "trust_workstation_source": true,
            "additional_trusted_sources": ["154.5.138.43/32"],
            "privileged": false,
            "enable_dynu_dns": true,
            "dynu_dns_domain": "${ dynu_dns_domain }"
          }
        }
      }
    }
  }
}