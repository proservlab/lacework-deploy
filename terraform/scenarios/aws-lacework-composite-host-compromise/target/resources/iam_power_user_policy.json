                    "ecs:List*",
                    "application-autoscaling:Describe*",
                    "events:Describe*",
                    "events:List*",
                    "events:Test*",
                    "elasticache:Describe*",
                    "elasticache:List*",
                    "es:Describe*",
                    "es:ESHttpGet",
                    "es:ESHttpHead",
                    "es:Get*",
                    "es:List*",
                    "eks:DescribeCluster",
                    "eks:ListClusters",
                    "kms:Describe*",
                    "kms:Get*",
                    "kms:List*",
                    "dynamodb:BatchGet*",
