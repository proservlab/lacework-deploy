{
  "context" :  {
    "global": {
      "environment"          : "target",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "iam" :  {
        "enabled" :  false,
        "user_policies_path" :  "${ iam_power_user_policy_path }",
        "users_path" :  "${ iam_users_path }"
      },
      "ec2" :  {
        "add_trusted_ingress" :  {
          "enabled"                     :  false,
          "trust_workstation"           :  false,
          "trust_attacker_source"       :  false,
          "trust_target_source"         :  false,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        }
      },
      "eks" :  {
        "add_iam_user_readonly_user": {
          "enabled": false
        }
      },
      "ssm" :  {
        "vulnerable" :  {
          "docker" :  {
            "log4shellapp" :  {
              "enabled" :  false
            }
          }
        },
        "ssh_keys" :  {
          "enabled" :  false
        }
      }
    },
    "kubernetes" :  {
      "aws": {
        "app" :  {
          "enabled" :  false
        },
        "psp" :  {
          "enabled" :  false
        },
        "vulnerable" :  {
          "log4shellapp" :  {
            "enabled" :  true,
            "image": "ghcr.io/credibleforce/log4shell-vulnerable-app"
          },
          "voteapp" :  {
            "enabled" :  false
          },
          "rdsapp" :  {
            "enabled" :  false
          },
          "privileged_pod" :  {
            "enabled" :  false
          },
          "root_mount_fs_pod" :  {
            "enabled" :  false
          }
        }
      },
      "gcp": {
        "app" :  {
          "enabled" :  false
        },
        "psp" :  {
          "enabled" :  false
        },
        "vulnerable" :  {
          "log4shellapp" :  {
            "enabled" :  false
          },
          "voteapp" :  {
            "enabled" :  false
          },
          "privileged_pod" :  {
            "enabled" :  false
          },
          "root_mount_fs_pod" :  {
            "enabled" :  false
          }
        }
      }
    }
  }
}