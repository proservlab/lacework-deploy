{
    "context" :  {
      "global": {
        "environment"          : "target",
        "deployment"           : "${ deployment }"
      },
      "aws" :  {
        "iam" :  {
          "enabled" :  true,
          "user_policies_path" :  "${ iam_power_user_policy_path }",
          "users_path" :  "${ iam_users_path }"
        },
        "ec2" :  {
          "add_trusted_ingress" :  {
            "enabled"                     :  true,
            "trust_workstation"           :  true,
            "trust_attacker_source"       :  true,
            "trust_target_source"         :  true,
            "additional_trusted_sources"  :  [],
            "trusted_tcp_ports"           :  {
              "from_port" :  0,
              "to_port" :  65535
            }
          },
          "add_app_trusted_ingress" :  {
            "enabled"                     :  true,
            "trust_workstation"           :  true,
            "trust_attacker_source"       :  true,
            "trust_target_source"         :  true,
            "additional_trusted_sources"  :  [],
            "trusted_tcp_ports"           :  {
              "from_port" :  0,
              "to_port" :  65535
            }
          }
        },
        "ssm" :  {
          "aws_credentials" :  {
            "enabled" :  true,
            "compromised_keys_user" : "khon.traktour@interlacelabs"
          },
          "ssh_keys" :  {
            "enabled" :  true
          },
          "vulnerable" : {
              "log4j_app" : {
                "enabled"                 : true,
                "listen_port"             : 8080
              },
              "rds_app" : {
                "enabled"                 : true,
                "listen_port"             : 8091
              }
            }
        }
      }
    }
  }