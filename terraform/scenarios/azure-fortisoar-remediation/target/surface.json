{
  "context" :  {
    "global": {
      "environment"          : "target",
      "deployment"           : "${ deployment }"
    },
    "azure" :  {
      "iam" :  {
        "enabled" :  true,
        "user_policies_path" :  "${ iam_power_user_policy_path }",
        "users_path" :  "${ iam_users_path }"
      },
      "runbook" :  {
        "azure_credentials" :  {
          "enabled" :  true,
          "compromised_keys_user" : "fortisoar-user-${deployment}"
        }
      }
    }
  }
}