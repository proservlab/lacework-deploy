{
    "context": {
        "global": {
            "environment"                           : "attacker",
            "deployment"                            : "${ deployment }"
        },
        "aws": {
            "region"        : "${ aws_region }",
            "profile_name"  : "${ aws_profile }",
            "ec2": {
                "enabled"                           : true,
                "public_network"                    : "172.28.0.0/16",
                "public_subnet"                     : "172.28.0.0/24",
                "public_app_network"                : "172.29.0.0/16",
                "public_app_subnet"                 : "172.29.0.0/24",
                "private_network"                   : "172.26.0.0/16",
                "private_subnet"                    : "172.26.100.0/24",
                "private_nat_subnet"                : "172.26.10.0/24",
                "private_app_network"               : "172.26.0.0/16",
                "private_app_subnet"                : "172.27.100.0/24",
                "private_app_nat_subnet"            : "172.27.10.0/24",
                "public_ingress_rules"              : [],
                "instances": [
                    {
                        "name"                      : "cloud1-rgv2-1",
                        "public"                    : true,
                        "role"                      : "default",
                        "instance_type"             : "t2.micro",
                        "ami_name"                  : "ubuntu_focal",
                        "tags": {
                          "ssm_deploy_lacework": "true",
                          "ssm_deploy_lacework_syscall": "true",
                          "ssm_exec_docker_log4shell_target" : "true"
                        },
                        "user_data"        : null,
                        "user_data_base64" : null
                      }
                ]
            },
            "ssm": {
                "enabled"                        : true,
                "deploy_git"                     : true,
                "deploy_docker"                  : true,
                "deploy_lacework_agent"          : true,
                "deploy_lacework_syscall_config" : true,
                "deploy_aws_cli"                 : true,
                "deploy_protonvpn_docker"        : true
            }
        },
        "lacework": {
            "aws_audit_config": {
                "enabled" : true
            }
        }
    }
}