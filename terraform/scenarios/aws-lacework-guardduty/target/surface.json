{
  "context" :  {
    "global": {
      "environment"          : "target",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "iam" :  {
        "enabled" :  true,
        "user_policies_path" :  "${ iam_power_user_policy_path }",
        "users_path" :  "${ iam_users_path }"
      },
      "ec2" :  {
        "add_trusted_ingress" :  {
          "enabled"                     :  true,
          "trust_workstation"           :  true,
          "trust_attacker_source"       :  true,
          "trust_target_source"         :  true,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        },
        "add_app_trusted_ingress" :  {
          "enabled"                     :  true,
          "trust_workstation"           :  true,
          "trust_attacker_source"       :  true,
          "trust_target_source"         :  true,
          "additional_trusted_sources"  :  [],
          "trusted_tcp_ports"           :  {
            "from_port" :  0,
            "to_port" :  65535
          }
        }
      },
      "ssm" :  {
        "vulnerable" :  {
          "docker" :  {
            "log4shellapp" :  {
              "enabled" :  false
            }
          },
          "rds_app" :  {
            "enabled" :  true
          }
        },
        "ssh_keys" :  {
          "enabled" :  true
        },
        "aws_credentials" :  {
          "enabled" :  true,
          "compromised_keys_user" : "reed.honalee@interlacelabs"
        }
      }
    }
  }
}