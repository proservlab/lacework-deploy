{
  "context" :  {
    "global": {
      "environment"          : "${ environment }",
      "deployment"           : "${ deployment }"
    },
    "aws" :  {
      "enabled" : true,
      "ssm" :  {
        "attacker": {
          "execute": {
            "docker_hydra" : {
              "enabled" :               true,
              "use_tor" :               false,
              "scan_local_network" :    false
            }
          }
        },
        "target": {
          "execute": {
            "docker_hydra" : {
              "enabled" :               true,
              "use_tor" :               false,
              "scan_local_network" :    false,
              "targets" : [
                "172.26.100.0/24",
                "172.18.0.0/24"
              ]
            }
          }
        }
      }
    }
  }
}